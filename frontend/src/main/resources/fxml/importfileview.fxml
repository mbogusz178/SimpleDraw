<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<Stage fx:id="stage" minHeight="100.0" minWidth="100.0" title="Wczytany obraz" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schumi178.javaprojects.graphics.zad1.ImportFileViewController">
    <scene>
        <Scene>
         <VBox minHeight="100.0" minWidth="100.0">
            <children>
               <MenuBar fx:id="menuBar">
                 <menus>
                   <Menu mnemonicParsing="false" text="Filtry">
                     <items>
                           <Menu fx:id="singleParamFilterMenu" mnemonicParsing="false" text="Filtry jednoparametrowe" />
                           <Menu fx:id="noParamFilterMenu" mnemonicParsing="false" text="Filtry bezparametrowe" />
                           <Menu fx:id="binarizationFilterMenu" mnemonicParsing="false" text="Binaryzacja" />
                           <Menu fx:id="morphoFilterMenu" mnemonicParsing="false" text="Filtry morfologiczne" />
                     </items>
                   </Menu>
                 </menus>
               </MenuBar>
               <StackPane fx:id="imageRoot" prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            </children>
         </VBox>
        </Scene>
    </scene>
</Stage>
